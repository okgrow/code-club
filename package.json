{
  "name": "code-club",
  "version": "1.0.0",
  "description": "JS Code Club Project Manager",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/okgrow/code-club.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/okgrow/code-club/issues"
  },
  "homepage": "https://github.com/okgrow/code-club#readme",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint --silent"
  },
  "eslintConfig": {
    "plugins": [
      "meteor"
    ],
    "extends": [
      "airbnb",
      "plugin:meteor/guide"
    ],
    "rules": {
      "no-console": 0,
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "^meteor/",
            "^/imports/"
          ]
        }
      ],
      "no-underscore-dangle": 0,
      "func-names": 0
    }
  },
  "dependencies": {
    "moment": "^2.13.0",
    "react": "^15.1.0",
    "react-addons-pure-render-mixin": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "devDependencies": {
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-meteor": "^3.6.0",
    "eslint-plugin-react": "^5.2.2"
  }
}
